{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "fractal-pattern-generator",
    "version": "0.0.0"
  },
  "tauri": {
    "systemTray": {
      "iconPath": "icons/128x128.png",
      "iconAsTemplate": true,
      "title": "fractal-pattern-generator"
    },
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "dialog": {
        "all": true
      },
      "fs": {
        "all": false,
        "copyFile": true,
        "createDir": false,
        "exists": true,
        "readDir": false,
        "readFile": true,
        "removeDir": false,
        "removeFile": false,
        "renameFile": true,
        "scope": [],
        "writeFile": true
      },
      "app": {
        "all": true
      }
    },
    "windows": [
      {
        "label": "main",
        "title": "fractal-pattern-generator",
        "url": "index.html",
        "theme": "Light"
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": false,
      "targets": "all",
      "identifier": "com.salam.fractal-pattern-generator",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
